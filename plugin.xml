<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova-plugin-videocall" version="0.0.1">
   <name>videocall</name>
   <js-module name="videocall" src="www/videocall.js">
      <clobbers target="cordova.plugins.videocall" />
   </js-module>
   <platform name="android">
      <config-file parent="/*" target="res/xml/config.xml">
         <feature name="videocall">
            <param name="android-package" value="cordova.plugin.videocall.videocall" />
         </feature>
      </config-file>
      <config-file parent="/*" target="AndroidManifest.xml" >
         <application android:name="com.twilio.video.app.VideoApplication" />
         <uses-permission android:name="android.permission.INTERNET" />
         <uses-feature android:name="android.hardware.camera"/>
         <uses-feature android:name="android.hardware.camera.autofocus"/>
         <uses-feature
            android:glEsVersion="0x00020000"
            android:required="true"/>

         <uses-permission android:name="android.permission.CAMERA"/>
         <uses-permission android:name="android.permission.RECORD_AUDIO"/>
         <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>
         <service
            android:foregroundServiceType="mediaProjection"
            android:name="com.twilio.video.app.ui.room.VideoService"/>
      </config-file>
